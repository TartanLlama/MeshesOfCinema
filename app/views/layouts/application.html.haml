!!! XML
!!!
%html
  %head
    %title Meshes of Cinema
    = stylesheet_link_tag "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
    = render "layouts/shim"
    
  %body
    #page
      = render 'layouts/header'
      #content_border
        #content
          %p.notice= notice
          %p.alert= alert
          = yield

    = render 'layouts/footer'


